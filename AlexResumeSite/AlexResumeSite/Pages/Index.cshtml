@page "/"
@model AlexResumeSite.Pages.HomeModel
@{
    <head>
        <link href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
        <link href="~/css/Home.css" rel="stylesheet">
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
        <script src="//code.jquery.com/jquery-1.11.1.min.js"></script> <!------ Include the above in your HEAD tag ---------->
    </head>
    <body>
        <nav class="main-navigation sticky" id="navbar">
            <div class="navbar-header animated fadeInUp">
                <a class="navbar-brand" href="#">Alexander Brewer</a>
            </div>
            <ul class="nav-list">
                <li class="nav-list-item">
                    <a href="Home" class="nav-link">Home</a>
                </li>
